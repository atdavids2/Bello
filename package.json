{
  "name": "Bello",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android-windows": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@types/react-native-calendars": "^1.20.6",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-navigation": "^3.0.7",
    "firebase": "^6.1.1",
    "react": "^16.8.6",
    "react-native": "^0.59.9",
    "react-native-calendars": "^1.185.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "@types/react-test-renderer": "^16.8.1",
    "babel-jest": "24.8.0",
    "jest": "24.8.0",
    "metro-react-native-babel-preset": "0.54.1",
    "react-addons-test-utils": "^15.6.2",
    "react-native-elements": "^1.1.0",
    "react-native-typescript-transformer": "^1.2.12",
    "react-native-vector-icons": "^6.5.0",
    "react-navigation": "^3.11.0",
    "react-test-renderer": "16.8.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
