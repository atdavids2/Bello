{
  "name": "Bellow",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android-windows": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "react": "16.5.0",
    "react-native": "0.57.1",
    "react-native-gesture-handler": "^1.0.12"
  },
  "devDependencies": {
    "@types/jest": "^23.3.4",
    "@types/react": "^16.4.16",
    "@types/react-native": "^0.57.4",
    "@types/react-test-renderer": "^16.0.3",
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.47.0",
    "react-addons-test-utils": "^15.6.2",
    "react-native-elements": "^0.19.1",
    "react-native-typescript-transformer": "^1.2.10",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^3.0.9",
    "react-test-renderer": "16.5.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
